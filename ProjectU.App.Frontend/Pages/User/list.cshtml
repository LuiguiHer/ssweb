@page
@model ProjectU.App.Frontend.Pages.ListUserModel
@{
}
 
<h1>User List</h1>
<table class="table">
  <thead>
     <tr>
      <th scope="col">ID</th>
      <th scope="col">NAME</th>
      <th scope="col">LASTNAME</th>
      <th scope="col">ADDRESS</th>
      <th scope="col">PHONE</th>
    </tr>
  </thead>
    @foreach (var User in Model.User)
    {
        <tr>
            <td>@User.id</td>
            <td>@User.Name</td>
            <td>@User.LastName</td>
            <td>@User.Address</td>
            <td>@User.Phone</td>
            <td>
              <a class="btn btn-success" asp-page="./Details" asp-route-busId="@User.id"><i class="fas fa-adjust"></i>Ver</a>
              <a class="btn btn-primary" href="">Editar</a>
              <a class="btn btn-danger" href="">Eliminar</a>
              </td>
        </tr>
    }
</table>