@page
@model ProjectU.App.Frontend.Pages.ListUserModel
@{
}
 
<h1>User List</h1>
<table class="table">
    @foreach (var User in Model.User)
    {
        <tr>
            <td>@User.id</td>
            <td>@User.Name</td>
            <td>@User.LastName</td>
            <td>@User.Address</td>
            <td>@User.Phone</td>
            <td>
              <a class="btn btn-info" asp-page="./Details" asp-route-busId="@bus.id"><i class="fas fa-adjust"></i>Ver</a>
              <a class="btn btn-info" href="">Editar</a>
              <a class="btn btn-danger" href="">Eliminar</a>
              </td>
        </tr>
    }
</table>